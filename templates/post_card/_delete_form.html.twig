<form method="post" action="{{ path('app_post_card_delete', {'id': post_card.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ post_card.id) }}">
    <button class="btn btn-outline-success">Delete</button>
</form>
